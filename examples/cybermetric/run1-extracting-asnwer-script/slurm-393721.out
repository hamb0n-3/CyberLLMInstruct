```python
# Python script using the socket library to perform a basic vulnerability scan (port scanner)

import socket

# Function to perform a TCP scan on a single host
def tcp_scan(ip, port):
    try:
        # Create a socket object
        s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        s.settimeout(1)  # Set a timeout for the connection attempt
        # Try to connect to the specified port
        result = s.connect_ex((ip, port))
        if result == 0:
            print(f"Port {port} is open on {ip}")
        s.close()
    except socket.error as err:
        print(f"Error on {ip}:{port} - {err}")

# Function to perform a UDP scan on a single host
def udp_scan(ip, port):
    try:
        # Create a socket object
        s = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)
        s.settimeout(1)  # Set a timeout for the connection attempt
        # Try to send a packet to the specified port
        result = s.connect_ex((ip, port))
        if result == 0:
            print(f"Port {port} is open on {ip}")
        s.close()
