# and sending them over the network.

def arp_request(ip_address):
    # Create a raw socket
    s = socket.socket(socket.AF_INET, socket.SOCK_RAW, socket.IPPROTO ARP)
    
    # Override with a specific source IP address if needed
    source_ip = '192.168.1.10'
    
    # Construct the ARP request packet
    # This is a simplified version and does not include error checking or ARP header
    arp_request_header = struct.pack('!HHddHHHH', 0x0806, 0x0001, socket.inet_aton(source_ip), 0, socket.inet_aton(ip_address), 0x0080, 0x0001)
    
    # Send the packet (this will require root privileges)
    s.sendto(arp_request_header, (ip_address, 0))
    
    # Receive the ARP response (this is a blocking operation)
    arp_response = s.recvfrom(4096)
    
    # Extract the MAC address from the response
    target_mac = arp_response[0][16:22]
    
    return target_mac

# Example usage
target_ip = '192.168.1.20'
target_mac = arp_request(target_ip)
print(f'The MAC address of {target_ip} is {target_mac.hex()}')

